{
  "name": "iterate-with-return-value",
  "version": "0.0.0-0",
  "description": "Gets the return value of an iterable after the for-of loop.",
  "author": "Microsoft Corporation",
  "homepage": "https://github.com/microsoft/conversational-ai-chat-sdk/",
  "repository": {
    "url": "https://github.com/compulim/conversational-ai-chat-sdk"
  },
  "license": "MIT",
  "files": [
    "/dist/"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./async": {
      "import": {
        "types": "./dist/index.async.d.mts",
        "default": "./dist/index.async.mjs"
      },
      "require": {
        "types": "./dist/index.async.d.ts",
        "default": "./dist/index.async.js"
      }
    }
  },
  "scripts": {
    "build": "tsup ./src/index.ts ./src/index.async.ts --dts --format cjs,esm --sourcemap --target chrome110,ios16,node18",
    "bump": "npm run bump:prod && npm run bump:dev",
    "bump:dev": "if [ `cat package.json | jq -r '(.devDependencies // {}) | length'` -ne 0 ]; then npm install $(cat package.json | jq -r '(.pinDependencies // {}) as $p | ((.devDependencies // {}) | keys) | map(. + \"@\" + ($p[.] // [\"latest\"])[0]) | .[]'); fi",
    "bump:prod": "if [ `cat package.json | jq -r '(.dependencies // {}) | length'` -ne 0 ]; then npm install $(cat package.json | jq -r '(.pinDependencies // {}) as $p | ((.dependencies // {}) | keys) | map(. + \"@\" + ($p[.] // [\"latest\"])[0]) | .[]'); fi",
    "eslint": "npm run precommit",
    "precommit": "eslint .",
    "prepack": "cp ../../LICENSE ./",
    "test": "jest --config jest.config.json"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "botframework-directlinejs": "^0.15.4",
    "lodash": "^4.17.21",
    "p-retry": "^5.1.2",
    "powerva-turn-based-chat-adapter-framework": "^0.0.1-0",
    "uuid": "^8.3.2"
  }
}
